<!-- 100-ssh-bruteforce.xml -->

<group name="local,ssh, authentication">
  <rule id="100100" level="3">
    <decoded_as>syslog</decoded_as>
    <field name="program">sshd</field>
    <match>Failed password</match>
    <description>SSH brute force (single event)</description>
    <group>authentication_failed,bruteforce,</group>
    <mitre>
      <id>T1110.001</id>
    </mitre>
  </rule>
</group>
