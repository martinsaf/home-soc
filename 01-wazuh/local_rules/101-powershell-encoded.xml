<!-- 101-powershell-enconded  -->

<group name="local,powershell,commandline,">
  <rule id="101101" level="10">
    <if_group>sysmon<if_group>
    <field name="win.system.eventID">1</field>
    <field name="win.system.eventdata.commandLine" type="pcre2">(?i)-[a-z]*enc</field>
    <description>PowerShell EncodedCommand detected (CommandLine contains -enc)</description>
    <group>powershell,execution,obfuscation,</group>
    <mitre>
      <id>T1059.001</id>
    </mitre>
  </rule>
</group>
